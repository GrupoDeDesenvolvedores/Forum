{% include 'template.twig' %}

{% block content %}
<div class="container">
    <div class="col-sm-12">
        <div class="card error">
            <div class="card-header">
                <h5>{{ error.statusCode }}</h5>
            </div>
            <div class="card-body">
                <p>{{ error.message }}</p>
            </div>
        </div>
    </div>
</div>
{% endblock %}